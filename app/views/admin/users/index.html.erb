  <div class="container">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">ユーザー名</th>
          <th scope="col">メールアドレス</th>
          <th scope="col">登録日</th>
          <th scope="col">管理</th>
        </tr>
      </thead>
      <% @users.each do |user| %>
      <tbody>
        <tr>
          <th scope="row">
            <%= user.name %>
          </th>
          <td><%= user.email %></td>
          <td><%= user.created_at.strftime('%Y/%m/%d') %></td>
          <td><%= link_to "削除", admin_user_path(user), method: :delete, class:"btn btn-danger", data: {confirm: "削除しますか？"} %></td>
        </tr>
      </tbody>
      <% end %>
    </table>
    <%= paginate @users %>
  </div>